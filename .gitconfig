[user]
	name = yxy
	email = yan5yang@gmail.com
[color]
	ui = true
[core]
	editor = vim
	excludesfile = /Users/yxy/.gitignore_global
    # pager = cat
[merge]
	tool = vimdiff
[alias]
	st = status
	ci = commit -s
	co = checkout
	br = branch
	unstage = reset HEAD
	plog = log --pretty=oneline --graph --stat
	glog = log --graph 
	sline = rev-list --oneline  --all
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
